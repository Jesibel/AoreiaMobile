<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
 xmlns:gap   = "http://phonegap.com/ns/1.0"
 id          = "com.aoreia.test"
 versionCode = "10" 
 version     = "1.0.0" >

 <!-- versionCode is optional and Android only -->

 <name>Aoreia Mobile</name>

 <description>
  An example for phonegap build docs. 
 </description> 

 <author href="http://test.aoreia.com" email="jesibelaranguren@gmail.com">
  Hardeep Shoker 
 </author>
 
 <content src="index.html" />

 <!-- Preferences -->

 <!--preference name="orientation" value="landscape" /-->
 <preference name="orientation" value="portrait" />


 <preference name="SplashScreen" value="splash" />
 <preference name="SplashScreenDelay" value="10000" /> <!-- ios: if set to false, the spinner won't appear on the splash screen during app loading -->
 <!--preference name="stay-in-webview" value="true" /--> 
 
 
 <!-- Define app icon for each platform. -->
  <icon src="resources/image/icon.png" />
  <icon src="resources/image/logo36.png"   gap:platform="android"    gap:density="ldpi" />
  <icon src="resources/image/logo48.png"   gap:platform="android"    gap:density="mdpi" />
  <icon src="resources/image/logo72.png"   gap:platform="android"    gap:density="hdpi" />
  <icon src="resources/image/logo96.png"   gap:platform="android"    gap:density="xhdpi" />
  
